  <ItemGroup>
    <PackageReference Include="Enchilada" Version="1.0.10" />
    <PackageReference Include="MassTransit" Version="5.1.4" />
  </ItemGroup>

    
  <PropertyGroup>
    <Title>MassTransit.MessageData.Enchilada</Title>
